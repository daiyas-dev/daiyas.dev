{
  "name": "daiyas_client",
  "version": "1.0.16",
  "description": "DaiyasClient For CTI",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron ."
  },
  "author": "NEW",
  "license": "MIT",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^8.3.0",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.2"
  },
  "dependencies": {
    "child-process": "^1.0.2",
    "electron-log": "^3.0.9",
    "electron-updater": "^4.2.0",
    "jquery": "^3.4.1",
    "rimraf": "^3.0.0"
  },
  "build": {
    "productName": "ダイヤスクライアント",
    "appId": "com.daiyas-foods.client",
    "nsis": {
      "include": "nsh/cti_shortcut.nsh"
    },
    "directories": {
      "output": "build"
